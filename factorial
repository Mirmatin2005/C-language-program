#include <stdio.h> 
#include <math.h>
int main()
{
    int n;
    printf("enter your number: ");
    scanf("%d", &n);
    if(n == 0){printf("1");}
    else if (n < 0){printf("invalid number");}
    else
    {
        int b = 1;
        int a = 1;
        while(n >= b)
        {
            a = a * b;
            b = b + 1;
        } 
        while(n <= b)
        {
            printf("\n%d\n", a);
            break;
        }   
    }
}
